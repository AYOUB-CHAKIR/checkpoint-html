algorithme"moyen general"
declaration:
    VAR:
    N[10]:reel;
    i:entier;
    M:reel

POUR i=0 à i<=9 pas(1)
    faire

    ecrire("saisir la note");
    lire(N[i]);
FINPOUR

M<-0
POUR i=0 à i<=9 pas(1)
    faire
    M<-M+N[i];
FINPOUR

ecrire("la moyen general est",M/10);
 
fin




algorithme"sup"
declaration: 
    var:
        N[10]:entier;
        M:entier;
        i:entier;
debut

pour i=0 à i<10 pas(1)
    faire
    ecrire("saisir le nombre N°",i);
    lire(N[i]);
FINPOUR

POUR i=0 à i<10 pas(1)
    faire
    si  N[i]>N[i+1]
        M<-N[i]
        SINON
        M<-N[i+1]
    FINSI
FINPOUR

ecrire("le nombre le plus grand est",M);

fin




