algorithme"categorie"
declaration:
    var:
        AGE:entier;
       
debut
    ecrire("saisir l'age de l'enfant");
    lire(AGE);
        
        SI(AGE=6 OU AGE=7);
            ecrire("poussin");
                SINON SI (AGE=8 ou AGE=9)
                ecrire("pupille");
                    SINON SI(AGE=10 ou AGE=11);
                    ecrire("minime"); 
                    SINON SI (AGE>=12);
                    ecrire("cadet");
        
        SINON
            ecrire("erreur");
                   
        FINSI
   

FIN



